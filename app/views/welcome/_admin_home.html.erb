<div class="row 2col">
	<div class="col-md-6 col-sm-12 spacing-bottom-sm spacing-bottom">
		<div class="tiles blue added-margin">
			<div class="tiles-body">
				<div class="controller">
					<a href="javascript:;" class="reload"></a>
					<a href="javascript:;" class="remove"></a>
				</div>
				<div class="tiles-title"> Number of Labs up to date </div>
				<div class="heading"> <span class="animate-number" data-value="<%=((@labs_gtr_6_months.count.to_f/@total_labs)*100).to_i%>" data-animation-duration="1200">
					<%=((@labs_gtr_6_months.count.to_f/@total_labs)*100).to_i%></span>%
				</div>
				<div class="progress transparent progress-small no-radius">
					<div class="progress-bar progress-bar-white animate-progress-bar" data-percentage="<%=((@labs_gtr_6_months.count.to_f/@total_labs)*100).to_i%>%" style="width: <%=((@labs_gtr_6_months.count.to_f/@total_labs)*100)%>%;">
					</div>
				</div>
				<div class="description">
					<span class="text-white mini-description ">&nbsp; Number of labs <span class="blend">where information updated > 6 months ago</span>
<%=link_to 'Click to View',labs_out_of_date_path%>
					</span>
			  </div>
			</div>
		</div>
	</div>
	<div class="col-md-6 col-sm-12 spacing-bottom-sm spacing-bottom">
		<div class="tiles green added-margin">
			<div class="tiles-body">
				<div class="controller">
					<a href="javascript:;" class="reload"></a>
					<a href="javascript:;" class="remove"></a>
				</div>
				<div class="tiles-title"> Number of Tests with UKAS Data </div>
				<div class="heading"> <span class="animate-number" data-value="<%=@lab_tests.count%>" data-animation-duration="1000"><%=@lab_tests.count.to_i%></span>
				</div>
				<div class="progress transparent progress-small no-radius">
					<div class="progress-bar progress-bar-white animate-progress-bar" data-percentage="<%=((@lab_tests.count.to_f/@total_lab_tests)*100).to_i%>%" style="width: <%=((@lab_tests.count.to_f/@total_lab_tests)*100).to_i%>%;"></div>
				</div>
				<div class="description">
					<span class="text-white mini-description ">&nbsp; Number of tests <span class="blend">with UKAS status out of <%=@total_lab_tests%></span>
				  </span>
				</div>
			</div>
		</div>
	</div>
<%= render 'widget', locals: {title: @title = 'Print Letters'}  %>

<div class="row">
	<% @laboratories.each_slice(3) do |row| %>
	<div class="col-md-6 col-sm-6">
		<% row.each_with_index do |laboratory, index| %>
		<div class="row ">
			<div class="col-md-12 m-b-10">
				<div class="widget-item ">
					<div class="controller overlay right"> <a href="javascript:;" class="reload"></a> <a href="javascript:;" class="remove"></a> </div>
					<%= render partial: "widget_header", locals: {laboratory: laboratory} %>
					<div class="tiles white ">
						<div class="tiles-body">
							
							<%= render partial: "widget_body", locals: {laboratory: laboratory} %>
						</div>
					</div>
				</div>
			</div>
		</div>
		<%end%>
	</div>
	<%end%>
</div>
<%= paginate @laboratories %>

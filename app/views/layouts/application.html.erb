<html lang="en" data-ember-extension="1"><head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8">
  <meta charset="utf-8">
  <title><%= yield :title %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
  <meta content="" name="description">
  <meta content="" name="author">

  <!-- BEGIN PLUGIN CSS -->
  <%= stylesheet_link_tag "pace-theme-flash", media: "all" %>
  <%= stylesheet_link_tag "bootstrap.min", media: "all" %>
  <%= stylesheet_link_tag "bootstrap-theme.min", media: "all" %>
  <%= stylesheet_link_tag "animate.min", media: "all" %>
  <%= stylesheet_link_tag "jquery.scrollbar", media: "all" %>
  <%= stylesheet_link_tag "application", media: "all" %>
  <!-- END PLUGIN CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">

  <!-- BEGIN CORE CSS FRAMEWORK -->
    <%= stylesheet_link_tag "webarch", media: "all" %>
    <!-- END CORE CSS FRAMEWORK -->
</head>
<body class="horizontal-menu">
  <%= render 'shared/header' %>
  <!-- BEGIN PAGE CONTAINER-->
    <div class="clearfix"></div>
    <!-- BEGIN CONTENT -->
    <div class="page-container row-fluid">
      <div class="page-content">
        <%= render 'shared/nav_bar' %>
        <div class="content">
      <div class="page-title">
        <h3><%= yield :content_title %></h3>
      </div>
      	<div class="row">
      	<div class="col-md-5">
          <%= yield %>
      	</div>
      	</div>
      </div>
    </div>
  </div>
  <!-- END CONTENT -->
  <!-- BEGIN CORE JS FRAMEWORK-->
  <%= javascript_include_tag "pace.min", "data-turbolinks-track" => true %>
  <!-- BEGIN JS DEPENDECENCIES-->

  <%= javascript_include_tag "jquery-1.11.3.min", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "bootstrap.min", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "jqueryblockui.min", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "jquery.unveil.min", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "jquery.scrollbar.min", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "jquery.animateNumbers", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "jquery.validate.min", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "select2.min", "data-turbolinks-track" => true %>
  <!-- END CORE JS DEPENDECENCIES-->
  <!-- BEGIN CORE TEMPLATE JS -->
  <%= javascript_include_tag "webarch", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "chat", "data-turbolinks-track" => true %>
  <!-- END CORE TEMPLATE JS -->
</body>
</html>
